<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="com.liferay.osb.provisioning.model.ProductBundle" />
	<import class="com.liferay.osb.provisioning.model.ProductBundleProducts" />
	<class dynamic-update="true" name="com.liferay.osb.provisioning.model.impl.ProductBundleImpl" table="Provisioning_ProductBundle">
		<id access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="productBundleId" type="long">
			<generator class="assigned" />
		</id>
		<version access="com.liferay.portal.dao.orm.hibernate.PrivatePropertyAccessor" name="mvccVersion" type="long" />
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" column="uuid_" name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="createDate" type="org.hibernate.type.TimestampType" />
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
	<class dynamic-update="true" name="com.liferay.osb.provisioning.model.impl.ProductBundleProductsImpl" table="Provisioning_ProductBundleProducts">
		<composite-id class="com.liferay.osb.provisioning.service.persistence.ProductBundleProductsPK" name="primaryKey">
			<key-property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="productBundleId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
			<key-property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="productKey" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		</composite-id>
		<version access="com.liferay.portal.dao.orm.hibernate.PrivatePropertyAccessor" name="mvccVersion" type="long" />
	</class>
</hibernate-mapping>