<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="com.liferay.osb.distributed.messaging.model.QueuedMessage" />
	<class dynamic-update="true" name="com.liferay.osb.distributed.messaging.model.impl.QueuedMessageImpl" table="DM_QueuedMessage">
		<id access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="queuedMessageId" type="long">
			<generator class="assigned" />
		</id>
		<version access="com.liferay.portal.dao.orm.hibernate.PrivatePropertyAccessor" name="mvccVersion" type="long" />
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="createDate" type="org.hibernate.type.TimestampType" />
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="messageBrokerClassName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="topic" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<one-to-one access="com.liferay.portal.dao.orm.hibernate.PrivatePropertyAccessor" cascade="save-update" class="com.liferay.osb.distributed.messaging.model.QueuedMessageMessageObjectBlobModel" constrained="true" name="messageObjectBlobModel" outer-join="false" />
	</class>
	<class dynamic-update="true" lazy="true" name="com.liferay.osb.distributed.messaging.model.QueuedMessageMessageObjectBlobModel" table="DM_QueuedMessage">
		<id column="queuedMessageId" name="queuedMessageId">
			<generator class="foreign">
				<param name="property">com.liferay.osb.distributed.messaging.model.impl.QueuedMessageImpl</param>
			</generator>
		</id>
		<property column="messageObject" name="messageObjectBlob" type="blob" />
	</class>
</hibernate-mapping>